    List<sp_LoadSHOWROOMList_Result> slSRID = ShowRoom.LoadSHOWROOMList();
    List<sp_LoadNHANVIEN_Result> slNVID = NhanVien.LoadStaff();
    KHACHHANG KHCODE = KhachHang.GetKHCODERecommend();
    // tạo KHCODE
    var codeTemp = Regex.Replace(KHCODE.KHCODE, "[^1-9]+", string.Empty);
    int intCode = codeTemp.ToInt();
    intCode = intCode + 1;
    var code = intCode.ToString();
    if (intCode.ToString().Length != 6)
    {
        for (int i = 0; i < 6 - intCode.ToString().Length; i++)
        {
            code = "0" + code;
        }
    }
    var finalCode = "AA" + code;
}
