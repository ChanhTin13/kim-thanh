@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/themes/default/style.min.css" />

<style>
    .was-validated input:required:valid, input:required.is-valid {
        border-color: #15af76;
        background-image: url('/App_themes/assets/images/btn-tick.png');
        background-repeat: no-repeat;
        background-position: right calc(.375em + .23438rem) center;
    }

    .was-validated input:required:invalid {
        border-color: #ff0000;
        background-image: url(/App_themes/assets/images/error-form.png);
        background-repeat: no-repeat;
        background-position: right calc(.375em + 0.1rem) center;
    }

    .table-khachhang tbody tr.selected td:nth-child(2) {
        color: #ffffff;
    }

    .table-khachhang tbody tr.selected td:nth-child(3) {
        color: #ffffff;
    }

    .pull-left {
        float: left !important;
    }

    .pull-right {
        float: right !important;
    }

    .table-nv tbody tr.selected td:nth-child(3) {
        color: #ffffff;
    }

    .table-nv tbody tr.selected td:nth-child(2) {
        color: #ffffff;
    }

    .disabled-dia-chi {
        pointer-events: none;
        opacity: 0.4;
    }

    .form-noi-bo-quan-li .select2-container--default {
        width: 100% !important
    }

    #form-phieu-thu .select2-container--default {
        width: 67% !important;
    }

    .disabled-phieu-thu {
        pointer-events: none;
        opacity: 0.4;
    }
</style>

<main class="wrapper">
    <!--Danh mục phiếu thu-->
    <div class="wrap-table">

        <div class="container-fluid">
            <div class="wrap-table__button">
                <div class="row">
                    <div class="col-md-12">
                        <div class="list-button text-left">
                            <a href="javascript:;" class="btn inline" id="btn-them-phieuthu">
                                <img src="../App_Themes/assets/images/btn-add.png" alt="" class="inline">
                                <span class="inline">Thêm</span>
                            </a>
                            <a href="javascript:;" class="btn inline" id="btn-edit-phieuthu">
                                <img src="../App_Themes/assets/images/btn-fix.png" alt="" class="inline"><span class="inline">Sửa</span>
                            </a>
                            <a href="javascript:;" class="btn inline" id="btn-delete-phieuthu">
                                <img src="../App_Themes/assets/images/btn-del.png" alt="" class="inline"><span class="inline">Xóa</span>
                            </a>
                            <a href="javascript:;" class="btn inline" id="btn-nap-phieuthu">
                                <img src="../App_Themes/assets/images/btn-import.png" alt="" class="inline"><span class="inline">Nạp</span>
                            </a>
                            <a href="javascript:;" class="btn inline" id="btn-print-phieuthu">
                                <img src="../App_Themes/assets/images/btn-print.png" alt="" class="inline"><span class="inline">In</span>
                            </a>
                            <a href="javascript:;" class="btn inline" id="btn-export-phieuthu">
                                <img src="../App_Themes/assets/images/btn-export.png" alt="" class="inline"><span class="inline">Xuất</span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="wrap-table__header">
                <div class="row">
                    <div class="col-md-12">
                        <div class="box-choose-time" id="date-phieuthu">
                            <p class="mr-2">Từ</p>
                            <input class="mr-2 datetimepicker date-only" name="chooseDate_Form" data-date-format="mm/dd/yyyy">
                            <p class="mr-2">Đến</p>
                            <input class="mr-2 datetimepicker date-only" name="chooseDate_To" data-date-format="mm/dd/yyyy">

                            @*<select name="" id="" class="mr-2">
                                    <option value="">Hôm nay</option>
                                    <option value="">Hôm sau</option>
                                    <option value="">Hôm bữa</option>
                                </select>*@

                            <button class="btn btn-trans" id="btn-searchdate-phieuthu">
                                <img src="../App_Themes/assets/images/loading.png" alt="" class="inline">
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="wrap-table__body">
                <div class="wrap-height-table">
                    <table class="table display" id="table_phieu_thu" style="width: 100%;">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Số phiếu</th>
                                <th>Lý do</th>
                                <th>Ngày thu</th>
                                <th>Nhân viên</th>
                                <th>Số tiền(VND)</th>
                                <th>Người nhận</th>
                                <th>Diễn giải</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                        <tfoot>
                            <tr>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!--Thêm phiếu thu-->
    <div class="modal fade default-popup popup-phieu-chi" id="form-phieu-thu"
         tabindex="-1"
         role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title title-popup" id="exampleModalLongTitle">Phiếu thu</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="box-border mt-3">
                        <p class="box-border__title bg-gray">Thông tin phiếu thu</p>
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="default-form">
                                        <div class="form-group">
                                            <label for=""></label>
                                            <div class="col-8 p-0 d-flex justify-content-between align-items-center">
                                                <p class="inline">
                                                    <b>Điều chỉnh tỉ giá USD</b>
                                                </p>
                                                <input type="text" name="pt-TiGia" data-type="currency" class="col-4 p-0 inline">
                                            </div>
                                        </div>
                                        <input type="hidden" name="pt-IDThu" />
                                        <div class="form-group">
                                            <label for="">Số phiếu</label>
                                            <input type="text" name="pt-SoPhieu" class="col-8 p-0" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Ngày thu</label>
                                            <input type="text" name="pt-Ngay" class="col-8 p-0 datetimepicker date-only" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Ca làm việc</label>
                                            <select name="pt-CaID" class="col-8 p-0" required></select>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Người thu</label>
                                            <div class="col-8 p-0">
                                                <select name="slNhanVien" id="slNhanVien" class="col-10" required></select>
                                                <span class="search-icon search-icon color-white absolute check-nhan-vien" id="btn-nhan-vien-phieu-thu">
                                                    <i class="fas fa-search"></i>
                                                </span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Chi nhánh</label>
                                            <select name="pt-SRID" class="col-8 p-0" required></select>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Loại tiền</label>
                                            <div class="col-8 p-0">
                                                <select name="pt-LoaiTien" class="col-5 p-0" required></select>
                                                <input type="text" name="pt-ValueLoaiTien" data-type="currency" class="col-5 p-0" disabled required>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Số tiền</label>
                                            <input type="text" name="pt-SoTien" data-type="currency" class="col-8 p-0" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Số tiền(VND)</label>
                                            <input type="text" name="pt-SoTienVND" data-type="currency" class="col-8 p-0" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Thuế</label>
                                            <input type="text" name="pt-Thue" @*data-type="currency"*@ class="col-8 p-0">
                                        </div>
                                        <div class="form-group">
                                            <label for="">Ngân hàng</label>
                                            <select name="pt-NHID" class="col-8 p-0"></select>
                                        </div>
                                        <div class="form-group">
                                            <label for="">Lý do thu</label>
                                            <div class="col-8 p-0">
                                                <select name="pt-LyDo" class="col-10" required></select>
                                                <span class="search-icon color-white absolute" id="btn-ly-do-phieu-thu" data-toggle="modal" data-target="#">
                                                    <i class="fas fa-search"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 default-form">
                                    <div class="list-btn">
                                        <button class="btn btn-success" id="btn-khach-hang-phieu-thu"> Đối tượng trả tiền: khách </button>
                                        @*<button class="btn btn-success"> KHCS nhận tiền </button>*@
                                    </div>
                                    <div class="box-border mt-2 disabled-phieu-thu">
                                        <input type="hidden" name="pt-IDNguoiNhan" class="col-8 p-0">
                                        <div class="form-group">
                                            <label for="">Mã người nhận</label>
                                            <input type="text" name="pt-CodeNguoiNhan" class="col-8 p-0">
                                        </div>
                                        <div class="form-group">
                                            <label for="">Tên người nhận</label>
                                            <input type="text" name="pt-NameNguoiNhan" class="col-8 p-0">
                                        </div>
                                        <div class="form-group">
                                            <label for="">Địa chỉ</label>
                                            <input type="text" name="pt-AddressNguoiNhan" class="col-8 p-0">
                                        </div>
                                        <div class="form-group">
                                            <label for="">Điện thoại</label>
                                            <input type="text" name="pt-PhoneNguoiNhan" class="col-8 p-0">
                                        </div>
                                    </div>
                                    <p class="color-red mt-2">
                                        <i>
                                            (Đối tượng nhận tiền ở đây sẽ không thực hiện cấn trừ công nợ)
                                        </i>
                                    </p>
                                    <div class="form-group mt-3">
                                        <label for="">Khoảng mục</label>
                                        <select name="pt-KhoanMuc" class="col-8 p-0">
                                        </select>
                                    </div>
                                    <div id="radio-ngan-hang">
                                        <div class="form-check mb-1">
                                            <label class="form-check-label">
                                                <input type="radio" class="form-check-input" value="0" name="ngan-hang"><span class="mr-1"> Rút từ ngân hàng về quỹ tiền mặt (rút)</span>
                                            </label>
                                        </div>
                                        <div class="form-check mb-1">
                                            <label class="form-check-label">
                                                <input type="radio" class="form-check-input" value="1" name="ngan-hang"><span class="mr-1"> Nộp vào ngân hàng (nộp) </span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="default-form">
                                        <div class="form-group">
                                            <label for="">Diễn giải</label>
                                            <textarea name="pt-DienGiai" class="col-10" rows="3"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <p class="color-red">
                                        <i>
                                            Chú ý: Nếu không chọn ngân hàng nào thì được hiểu là chi tiền mặt từ sổ quỷ
                                        </i>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" id="btn-save-phieuthu">Lưu</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Danh mục nhân viên -->
    <div class="modal fade default-popup popup-danhsach-nv" id="danh-sach-nv"
         tabindex="-1"
         role="dialog"
         aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title title-popup" id="exampleModalLongTitle">
                        Danh
                        mục nhân viên
                    </h5>
                    <button type="button" class="close" data-dismiss="modal"
                            aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="ql-user user-table wrap-table-pq">
                                    <div class="user-table__header">
                                        <div class="row justify-content-end">
                                            <div class="col-md-7">
                                                <div class="list-button text-left">
                                                    <a href="#" class="btn btn-add inline" id="btn-insert-nhan-vien">
                                                        <img src="~/App_Themes/assets/images/btn-add.png"
                                                             alt="" class="inline">
                                                        <span class="inline">Thêm</span>
                                                    </a>
                                                    <a href="#" class="btn btn-fix inline" id="btn-update-nhan-vien">
                                                        <img src="~/App_Themes/assets/images/btn-fix.png"
                                                             alt="" class="inline"><span class="inline">Sửa</span>
                                                    </a>
                                                    <a href="#" class="btn btn-del inline" id="btn-delete-nhan-vien">
                                                        <img src="~/App_Themes/assets/images/btn-del.png"
                                                             alt="" class="inline"><span class="inline">Xóa</span>
                                                    </a>
                                                    <a href="#" class="btn btn-export inline" id="btn-export-nhan-vien">
                                                        <img src="~/App_Themes/assets/images/btn-export.png"
                                                             alt="" class="inline"><span class="inline">Xuất</span>
                                                    </a>
                                                    <a href="#" class="btn btn-import inline" id="btn-reset-nhan-vien">
                                                        <img src="~/App_Themes/assets/images/btn-import.png"
                                                             alt="" class="inline"><span class="inline">Nạp</span>
                                                    </a>
                                                    <a href="#excel-nhanvien" data-toggle="modal" class="btn btn-export inline">
                                                        <img src="/App_Themes/assets/images/btn-import-2.png"
                                                             alt="" class="inline"><span class="inline">Nhập</span>
                                                    </a>
                                                    @*<a href="#" class="btn btn-print inline" id="btn-print-nhan-vien">
                                                            <img src="~/App_Themes/assets/images/btn-print.png"
                                                                 alt="" class="inline"><span class="inline">In</span>
                                                        </a>
                                                        <a href="#" class="btn btn-close inline">
                                                            <img src="~/App_Themes/assets/images/btn-close.png"
                                                                 alt="" class="inline"><span class="inline">Đóng</span>
                                                        </a>
                                                        <a href="#" class="btn btn-question inline">
                                                            <img src="~/App_Themes/assets/images/question.png"
                                                                 alt="" class="inline"><span class="inline">Hỏi</span>
                                                        </a>*@
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="wrap-height-table">
                                        <table class="table display nowrap row-border order-column table-striped table-fixed table-nv" style="width:100%" id="table-nv">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>Mã NV</th>
                                                    <th>
                                                        Tên NV
                                                    </th>
                                                    <th>
                                                        Địa chỉ
                                                    </th>
                                                    <th>
                                                        Điện thoại
                                                    </th>
                                                    <th>
                                                        Ngày sinh
                                                    </th>
                                                    <th>
                                                        Giới tính
                                                    </th>
                                                    <th>
                                                        Chức vụ
                                                    </th>
                                                    <th>
                                                        Mức lương
                                                    </th>
                                                    <th>
                                                        CMTND
                                                    </th>
                                                    <th>
                                                        Theo dõi
                                                    </th>
                                                    <th>
                                                        Người QL
                                                    </th>
                                                    <th>
                                                        Ghi chú
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Thêm nhân viên -->
    <div class="modal fade default-popup popup-them-nv" id="them-nhan-vien" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title title-popup" id="exampleModalLongTitle">Thêm/sửa nhân viên</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6">
                                <form class="form-2-col" id="staff-info">
                                    <input type="hidden" name="idNV" />
                                    <div class="form-row">
                                        <label for="">Mã NV</label>
                                        <input type="text" name="maNV" required>
                                        <div id="msg-staff"></div>
                                    </div>
                                    <div class="form-row">
                                        <label for="">Tên NV</label>
                                        <input type="text" name="tenNV" required>
                                    </div>
                                    <div class="form-row">
                                        <label for="">Ngày sinh</label>
                                        <input type="text" class="datetimepicker datepicker date-only" name="ngaySinhNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Giới tính</label>
                                        <select name="slGioiTinhNV">
                                            <option value="1">Nam</option>
                                            <option value="2">Nữ</option>
                                        </select>
                                    </div>
                                    <div class="form-row">
                                        <label for="">Chức vụ</label>
                                        <input type="text" name="chucVuNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Lương</label>
                                        <input type="text" data-type="currency" name="luongNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Số CMND</label>
                                        <input type="text" data-type="number" name="soCMNDNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Địa chỉ</label>
                                        <input type="text" name="diaChiNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Điện thoại</label>
                                        <input type="text" data-type="number" name="dienThoaiNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Di động</label>
                                        <input type="text" data-type="number" name="diDongNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Email</label>
                                        <input type="text" name="emailNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Làm việc tại CN</label>
                                        <select name="slShowRoom">
                                            <option value="0">--Chọn chi nhánh--</option>
                                        </select>
                                    </div>
                                    <div class="form-row">
                                        <label for="">Nhóm</label>
                                        <select name="slNhomNhanVien">
                                            <option value="0">--Chọn nhóm nhân viên--</option>
                                        </select>
                                        <span class="search-icon" id="btn-nhom-nhan-vien">
                                            <i class="fas fa-search"></i>
                                        </span>
                                    </div>
                                    <div class="form-row">
                                        <label for="">Quê quán</label>
                                        <input type="text" name="queQuanNV">
                                    </div>
                                    <div class="form-row">
                                        <label for="">Gia đình</label>
                                        <input type="text" name="giaDinhNV">
                                    </div>
                                    @*<div class="form-row">
                                            <label for="">Theo dõi</label>
                                            <input type="checkbox" class="editor-active" name="cbFollow" />
                                        </div>*@
                                </form>
                            </div>
                            <div class="col-md-6">
                                <div class="box-up-img text-center">
                                    <h2>Up ảnh cá nhân tại đây</h2>
                                    <div class="avatar-upload">
                                        <div class="avatar-edit">
                                            <input type='file' id="imageUpload"
                                                   accept=".png, .jpg, .jpeg" />
                                            <label for="imageUpload">
                                                <i class="fas fa-cloud-upload-alt"></i>
                                            </label>
                                        </div>
                                        <div class="avatar-preview">
                                            <div id="imagePreview"
                                                 style="background-image: url('../App_Themes/assets/images/rose.jpg');">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="form-row mr-auto">
                        <label for="">Theo dõi</label>
                        <input type="checkbox" class="editor-active" name="theoDoiNV" />
                    </div>
                    <button type="button" class="btn btn-primary" id="btn-save-nhan-vien">Lưu</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
                </div>
            </div>
        </div>
    </div>
    <!-- Danh mục nhóm nhân viên -->
    <div class="modal fade default-popup popup-them-nhom-nv" id="them-nhom-nv" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title title-popup" id="exampleModalLongTitle"> Danh mục nhóm nhân viên </h5>
                    <button type="button" class="close" data-dismiss="modal"
                            aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="list-button text-left">
                        <a href="#" class="btn btn-add inline" id="btn-insert-nhom-nhan-vien">
                            <img src="~/App_Themes/assets/images/btn-add.png" alt=""
                                 class="inline"> <span class="inline">Thêm</span>
                        </a>
