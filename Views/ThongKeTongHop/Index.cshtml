        function removeBC(name) {
            if (name === 'sokho') {
                $("#sokho").empty();
            } else if (name === 'tonkho') {
                $("#tonkho").empty();
            } else if (name === 'hethang') {
                $("#hethang").empty();
            } else if (name === 'the-kho-chi-tiet') {
                $("#the-kho-chi-tiet").empty();
            } else if (name === 'cong-no-phai-thu-tra') {
                $("#cong-no-phai-thu-tra").empty();
            } else if (name === 'bao-cao-ban-hang') {
                $("#bao-cao-ban-hang").empty();
            } else if (name === 'so-cskh') {
                $("#so-cskh").empty();
            } else if (name === 'quan-li-han-the') {
                $("#quan-li-han-the").empty();
            } else if (name === 'thu-chi-chot-ca') {
                $("#thu-chi-chot-ca").empty();
            } else if (name === 'quan-li-serial') {
                $("#quan-li-serial").empty();
            } else if (name === 'tang-truong-doanh-so') {
                $("#tang-truong-doanh-so").empty();
            } else if (name === 'theo-doi-hang-ton') {
                $("#theo-doi-hang-ton").empty();
            } else if (name === 'loi-nhuan') {
                $("#loi-nhuan").empty();
            }
        }

        $('button[data-bc]').on('click', function () {

            var data_bc = $(this).attr('data-bc');
            var prev = getPrev(data_bc)[0];
            removeBC(prev);

            if (prev != undefined) {
                var scripts = document.getElementsByTagName('script');
                for (var i = 0; i < scripts.length; i++) {
                    if (scripts[i].className == "js-baocao-chitiet") {
                        scripts[i].parentNode.removeChild(scripts[i]);
                        i--;
                    }
                }
            }

            if (data_bc === 'sokho') {
                SoKho();
            }
            else if (data_bc === 'tonkho') {
                TonKho();
            }
            else if (data_bc == 'hethang') {
                HetHang();
            }
            else if (data_bc == 'loi-nhuan') {
                LoiNhuan();
            }
            else if (data_bc == 'bao-cao-ban-hang') {
                BaoCaoBanHang();
            }
            else if (data_bc == 'thu-chi-chot-ca') {
                ThuChiChotCa();
            }
            else if (data_bc == 'the-kho-chi-tiet') {
                TheKhoChiTiet();
            }
            else if (data_bc == 'theo-doi-hang-ton') {
                TheoDoiHangTon();
            }
        });

        let arr = new Array();
        let remove = new Array();
        function getPrev(attr) {
            if (arr.length > 0) {
                if (arr.length == 1) {
                    arr.push(attr);
                    return remove;
                }
                else {
                    remove = arr.splice(1, 1);
                    arr.push(attr);
                    return remove;
                }
            }
            else {
                arr.push(attr);
                remove.push(attr);
                return [];
            }
        };
