    <script>
        $(document).ready(function () {
            if ('@TempData["AlertMessage"]' != '') {
                toast.create({
                    title: 'Notification!',
                    text: '@TempData["AlertMessage"]',
                    icon: '@TempData["Icon"]',
                    classBackground: '@TempData["Class"]',
                    timeout: 3000
                })
            } $('[data-toggle="tooltip"]').tooltip();
            $(document).ajaxStart(function () {
                $("#AjaxLoader").show();
            });
            $(document).ajaxStop(function () {
                $("#AjaxLoader").hide();
            });

        });

    </script>
    <script>
        $('#btn-change-password').click(function () {
            let oldpd = $('input[name="old-password"]').val();
            let newpd = $('input[name="new-password"]').val();
            let comfirmpd = $('input[name="comfirm-password"]').val();
            let $currentForm = $('#form-doi-mat-khau');
            let inputs = $currentForm.find('*:required');
            for (let i = 0; i < inputs.length; i++) {
                if (inputs[i].value === "") {
                    $currentForm.addClass('was-validated');
                    return false;
                    break;
                }
            }
            $.ajax({
                type: "POST",
                url: "/Login/ChangePassword",
                data: '{OldPd: "' + oldpd + '", NewPd: "' + newpd + '",ComfirmPd: "' + comfirmpd + '"  }',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    if (msg.status == 1) {
                        $('input[name="old-password"]').val("");
                        $('input[name="new-password"]').val("");
                        $('input[name="comfirm-password"]').val("");
                        $('#form-doi-mat-khau').modal('hide');
                        toast.create({
                            title: 'Thông Báo!',
                            text: msg.message,
                            icon: 'check',
                            classBackground: 'noti-success',
                            timeout: 3000
                        });
                    } else {
                        toast.create({
                            title: 'Thông Báo!',
                            text: msg.message,
                            icon: 'error_outline',
                            classBackground: 'noti-error',
                            timeout: 3000
                        })
                    }
                },
                error: function (error) {
                    console.log('e');
                }

            })
        });
        $('.sao-luu-phuc-hoi').click(function () {
            $.ajax({
                type: "GET",
                url: "/Login/BackupData",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {

                    if (msg.status == 1) {
                        toast.create({
                            title: 'Thông Báo!',
                            text: msg.message,
                            icon: 'check',
                            classBackground: 'noti-success',
                            timeout: 3000
                        });
                    } else {
                        toast.create({
                            title: 'Thông Báo!',
                            text: msg.message,
                            icon: 'error_outline',
                            classBackground: 'noti-error',
                            timeout: 3000
                        })
                    }
                },
                error: function (error) {
                    console.log('e');
                }

            })
        });
    </script>
